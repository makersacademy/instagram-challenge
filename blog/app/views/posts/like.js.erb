$(.'liked-btn').on('ajax:success', function(){
    $(this).parent().parent().find('.like_count')
    .html('<%= @post.get_likes.size %> &#128591');
    $(this).closest('.liked-btn').html('Like');
});
$(.'like-btn').on('ajax:success', function(){
    $(this).parent().parent().find('.like_count')
    .html('<%= @post.get_likes.size %> &#128591');
    $(this).closest('.like-btn').html('Liked');
});
