<!doctype html>
<html>
<head>
  <title>Instagram | Share your snaps!</title>
</head>

<body>
<%= form_for @picture, url: "/new", html: { multipart: true } do |form| %>
  <%= form.label(:description, "description:") %>
  <%= form.text_field(:description) %>
  <%= form.label(:image, "image:") %>
  <%= form.file_field(:image) %>
  <%= form.submit("Add") %>
<% end %>

</body>
</html>
