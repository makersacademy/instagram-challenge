.col-md-8.col-md-offset-2
  .row
    .panel.panel-default
      .panel-heading
        %h1 Post Photo
      .panel-body
        = form_for @photo, html: { multipart: true } do |f|
          =f.file_field :image
          =f.label :caption
          =f.text_field :caption
          =f.submit 'Shoot'
