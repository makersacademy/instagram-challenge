
<div class="photo-wrapper row">
<% @photos.each do |photo| %>
<div class="photo">
  <div class="photo-head">
    <div class="name">
      <%= photo.user.user_name %>
    </div>
  </div>
<div class="image center-block">
<%= link_to (image_tag photo.image.url (:medium), class: 'img-responsive'),photo_path(photo) %>
</div>
<p class="caption">
<%= photo.caption %>
</p>
</div>
  <% end %>
</div>
