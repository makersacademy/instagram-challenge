
<div class = "form-wrapper">
<%= form_for @photo, html: {multipart: true}  do |f| %>
  <%= render 'shared/errors', object: @photo %>
  <div class = "form-group row">
   	<%= f.file_field :image %>
	</div>
	 <div class = "form-group row">
  <%= f.label "description" %>
  <%= f.text_field :description, :class => 'form-control' %>
  </div>
   <div class = "form-group row">
  <%= f.submit :class => 'btn-primary' %>
  </div>
<% end %>
</div>


