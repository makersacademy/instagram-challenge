<p id="notice"><%= notice %></p>

<h1>Grams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <% @grams.each do |gram| %>
          <td><%= link_to image_tag(gram.photo.small.url), gram %></td>
      <% end %>
    </tr>
  </tbody>
</table>

<br>

<%= link_to 'New Gram', new_gram_path %>
