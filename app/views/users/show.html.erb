<body>
  <div class="topnav">
    <h2>Eternagram</h2><br />
    <%= link_to('Log Out', destroy_user_session_path, method: :delete) %>
    <%= link_to('Home', posts_url, method: :get) %>
  </div>
  <h1><%= @user.username %></h1>
  <p>This is the profile of <%= @user.username %></p>

  <% @posts.each do |post| %>
    <%= image_tag(post.photo, :size => '260x180') %><br />
  <% end %>
</body>
