<h1>New user page</h1>

  <%= form_for @user do |f|  %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :password %>
    <%= f.text_field :password %><br>
    <%= f.submit "Create Account" %> 
  <% end %>