<h1>New Post</h1>

<%= form_for @post, html: { multipart: true } do |f| %>

  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div><br />

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div><br />

  <div class="actions">
    <%= f.submit :Post%>
  </div>

<% end %>
