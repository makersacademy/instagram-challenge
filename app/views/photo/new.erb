Add new photo to Sofigram!
<%= form_for :photo, url: '/photo', method: :post, multipart: true do |f| %>
    <%= f.label :description %>
    <%= f.text_area :description, placeholder: "Description", class: "form-control"  %><br>
    <%= f.label :image %>
    <%= f.file_field :image, class: "form-control" %>
    <br/>

    <%= f.submit("Add", class: "btn btn-default center-block") %>

<% end %>
