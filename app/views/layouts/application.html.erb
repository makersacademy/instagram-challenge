<!DOCTYPE html>
<html>
<head>
  <title>InstagramChallenge</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div>
  <% flash.each do |key, value| %>
    <%= key.capitalize%>,
      <%= value %>
    </div>
  <% end %>

<%= yield %>

</body>
</html>
