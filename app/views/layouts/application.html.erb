<!DOCTYPE html>
<html>
<head>
  <title>J-Instagram</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'navbar', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'user', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'photos', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Cookie' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Clicker+Script" />

<body>
  <%= render "/layouts/navbar" %>
  <section class="container">
    <%= notice %>
    <%= yield %>
  </section>
</body>
</html>
