<% puts (params[:photo_id]) %>
<% if (params[:photo_id]) %>
  <%= form_for [ @photo, @comment ] do |f| %>
    <%= f.label :comment %>
    <%= f.text_field :body %>
    <%= f.submit %>
  <% end %>
  <% else %>
  <%= form_for  [@commentable, Comment.new] do |f| %>
    <%= f.label :comment %>
    <%= f.text_field :body %>
    <%= f.submit %>
  <% end %>
<% end %>
