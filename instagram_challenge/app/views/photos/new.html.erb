<%= form_for @photo, html: {multipart: true} do |f| %>
  <%= f.label :caption %>
  <br>
  <%=f.text_field :caption %>
  <br>
  Select image:
  <br>
  <%= f.file_field :image %>
  <br>
  <br>
  <%=f.submit %>
  <br>
<%end%>
